{
  "bios_config": [
    {
      "pattern": {
        "plan": ".*",
        "os": ".*"
      },
      "settings": {
        "SecureBootEnable": false,
        "IntelVirtualizationTechnology": "Enabled",
        "NetworkStack": "Enabled",
        "IPv4PXESupport": "Enabled"
      }
    },
    {
      "pattern": {
        "plan": "n3\\..*"
      },
      "settings": {
        "c3": "Enabled"
      }
    },
    {
      "pattern": {
        "os": "ubuntu.*"
      },
      "settings": {
        "NetworkStack": "Enabled",
        "IPv4PXESupport": "Enabled"
      }
    },
    {
      "pattern": {
        "plan": "ms5\\..*",
        "os": "ubuntu.*"
      },
      "settings": {
        "TurboMode": "Enabled",
        "PowerPerformanceTuning": "Performance"
      }
    }
  ],
  "post_bios_config": [
    {
      "pattern": { "os": ".*" },
      "settings": {
        "SecureBootEnable": true,
        "SecureBootMode": "Standard"
      }
    }
  ]
}
